{
    "manifest_version": 2,
    "name": "NeoSearch favicons",
    "version": "2.2",
    "author": "Ilian Delagrange",
    "description": "Adds favicons next to the results of search engines that do not support them natively. List of current supported engines : SearXNG",

    "icons": {
        "171": "/data/icons/square.png",
        "128": "/data/icons/square-128.png",
        "64": "/data/icons/square-64.png",
        "32": "/data/icons/square-32.png",
        "16": "/data/icons/square-16.png"
    },

    "browser_specific_settings": {
        "gecko": {
            "id": "neosearch@ex.com",
            "strict_min_version": "79.0"
        },
        "gecko_android": {
            "id": "neosearch@ex.com",
            "strict_min_version": "113.0"
        }
    },

    "options_ui": {
        "page": "./options-page/options.html",
        "browser_style": false,
        "open_in_tab": false
    },

    "browser_action": {
        "default_icon": {
            "171": "/data/icons/square.png",
            "128": "/data/icons/square-128.png",
            "64": "/data/icons/square-64.png",
            "32": "/data/icons/square-32.png",
            "16": "/data/icons/square-16.png"
        },
        "default_title": "NeoSearch favicons",
        "default_popup": "./options-page/options.html"
    },

