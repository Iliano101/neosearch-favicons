{
    "manifest_version": 2,
    "name": "NeoSearch favicons",
    "version": "1.5",
    "author": "Ilian Delagrange",

    "description": "Adds favicons next to the results of search engines that do not support them natively. List of current supported engines : SearXNG",

    "background": {
        "scripts": ["./scripts/background.js"]
    },

    "icons": {
        "171": "icons/square.png",
        "128": "icons/square-128.png",
        "64": "icons/square-64.png",
        "32": "icons/square-32.png",
        "16": "icons/square-16.png"
    },

    "options_ui": {
        "page": "./options-page/options.html",
        "browser_style": false,
        "open_in_tab": false
    },

    "browser_action": {
        "default_icon": {
            "171": "icons/square.png",
            "128": "icons/square-128.png",
            "64": "icons/square-64.png",
            "32": "icons/square-32.png",
            "16": "icons/square-16.png"
        },
        "default_title": "NeoSearch favicons",
        "default_popup": "./options-page/options.html"
    },

